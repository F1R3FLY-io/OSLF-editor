{
	"compilerOptions": {
		"lib": ["ES2017", "DOM"],
		"outDir": "dist/",
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
